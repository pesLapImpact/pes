<table class="table table-horizontal table-bordered">
  <thead class="thead-strong">
    <tr>
      <th>Home Team</th>
      <th>Score</th>
      <th>vs</th>
      <th>Score</th>
      <th>Away Team</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody id="tbody">
  {{#each member_list}}
    <tr>
      <td>{{home_team}}</td>
      <td>{{home_score}}</td>
      <td>vs</td>
      <td>{{away_score}}</td>
      <td>{{away_team}}</td>
      <td>{{date}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>