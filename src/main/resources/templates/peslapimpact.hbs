<!DOCTYPE HTML>
<html>
<head>
  <title>PES Lap Impact Fan Page</title>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />

  <!-- add css -->
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css//common.css">
  <link rel="stylesheet" href="/css//peslapimpact.css">
  <link rel="stylesheet" href="/css//header.css">
</head>
<body>
  <div id="header"></div>
  <div id="content">
    <div id="data_panel"> 
      <div id="data_header" class="col-md-12 data_title"><strong>#PES LAP IMAPACT </strong>MAIN PAGE</div>
      <div id="data_content" class="col-md-12">
        <div id="news_content shadow" class="col-xl-3 col-md-6 mb-4">
          <div><strong>#News</strong> Top5</div>
          <table class="table table-striped">
            <thead class="thead-strong">
              <tr>
                <th>No</th>
                  <th>News Title</th>
                  <th>Date</th>
                </tr>
            </thead>
            <tbody id="tbody">
            {{#each news_list}}
              <tr>
                <td>{{id}}</td>
                <td>{{news_title}}</td>
                <td>{{date}}</td>
              </tr>
            {{/each}}
            </tbody>
          </table>        
        </div>
        <div id="member_ranking_content" class="col-xl-3 col-md-6 mb-4">
          <div><strong>#Member Ranking</strong> Top5</div>
          <table class="table table-striped">
            <thead class="thead-strong">
              <tr>
                <th>No</th>
                  <th>News Title</th>
                  <th>Date</th>
                </tr>
            </thead>
            <tbody id="tbody">
            {{#each news_list}}
              <tr>
                <td>{{id}}</td>
                <td>{{news_title}}</td>
                <td>{{date}}</td>
              </tr>
            {{/each}}
            </tbody>
          </table>        
        </div>
        <div id="team_ranking_content" class="col-xl-3 col-md-6 mb-4">
          <div><strong>#Team Ranking</strong> Top5</div>
          <table class="table table-striped">
            <thead class="thead-strong">
              <tr>
                <th>No</th>
                  <th>News Title</th>
                  <th>Date</th>
                </tr>
            </thead>
            <tbody id="tbody">
            {{#each news_list}}
              <tr>
                <td>{{id}}</td>
                <td>{{news_title}}</td>
                <td>{{date}}</td>
              </tr>
            {{/each}}
            </tbody>
          </table>        
        </div>
        <div id="member_score_content" class="col-xl-3 col-md-6 mb-4">
          <div><strong>#Member Score</strong> Top5</div>
          <table class="table table-striped">
            <thead class="thead-strong">
              <tr>
                <th>No</th>
                  <th>News Title</th>
                  <th>Date</th>
                </tr>
            </thead>
            <tbody id="tbody">
            {{#each news_list}}
              <tr>
                <td>{{id}}</td>
                <td>{{news_title}}</td>
                <td>{{date}}</td>
              </tr>
            {{/each}}
            </tbody>
          </table>        
        </div>
        <div id="team_score_content" class="col-xl-3 col-md-6 mb-4">
          <div><strong>#Team Score</strong> Top5</div>
          <table class="table table-striped">
            <thead class="thead-strong">
              <tr>
                <th>No</th>
                  <th>News Title</th>
                  <th>Date</th>
                </tr>
            </thead>
            <tbody id="tbody">
            {{#each news_list}}
              <tr>
                <td>{{id}}</td>
                <td>{{news_title}}</td>
                <td>{{date}}</td>
              </tr>
            {{/each}}
            </tbody>
          </table>        
        </div>
      </div>
      <div id="data_footer" class="data_title"><strong>PES</strong> LAP IMPACT BOARD</div>
    </div>
  </div>
  <div id="footer"></div>
  <!-- add js, jquery lib -->
  <script src="/js/lib/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>

  <!--add custom js -->
  <script src="/js/app/common.js"></script>
  <script src="/js/app/peslapimpact.js"></script>
</body>
</html>